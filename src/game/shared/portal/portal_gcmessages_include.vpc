$Project
{
	$Folder "Source Files"
	{
		$File "$SRCDIR\game\shared\portal\portal_gcmessages.cpp"
	}

	$Folder "Header Files"
	{
		$File "$SRCDIR\game\shared\portal\portal_gcmessages.h"
		$File "$SRCDIR\game\shared\portal\portal_matchmaking_scoring.h"
	}

	$Folder "Protobuf Files"
	{
		$File "$SRCDIR\game\shared\portal\portal_gcmessages.proto"
		$DynamicFile "$GENERATED_PROTO_DIR\portal_gcmessages.pb.h"
		$DynamicFile "$GENERATED_PROTO_DIR\portal_gcmessages.pb.cc" [!$OSXALL]
		{
			$Configuration
			{
				$Compiler
				{
					$Create/UsePrecompiledHeader	"Not Using Precompiled Headers"
				}
			}
		}
	}

}
